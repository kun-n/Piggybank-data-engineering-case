CREATE TABLE household (
    HOUSEHOLD_ID   INTEGER NOT NULL,
    INCOME_ID      INTEGER NOT NULL,
    REPORTING_DATE TEXT    NOT NULL,
    MARRIED        TEXT    NOT NULL,
    HOUSE_OWNER    TEXT    NOT NULL,
    BUCKET         INTEGER NOT NULL,
    CHILD_NO       INTEGER NOT NULL,
    HH_MEMBERS     INTEGER NOT NULL,
    PRIMARY KEY (HOUSEHOLD_ID, BUCKET),
    FOREIGN KEY (INCOME_ID, BUCKET) REFERENCES income(INCOME_ID, BUCKET) ON DELETE CASCADE ON UPDATE CASCADE
    );   